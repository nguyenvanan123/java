package btthuchanh;

import java.util.Scanner;

public class inracacsochan0den10tru4 {
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);

        for (int i = 0; i < 11; i++){
            if ((i % 2 == 1) || (i == 4))
                continue;
                System.out.println(i);

            }
    }
}



